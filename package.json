{
  "name": "hoppingmode-frontend",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.ts --port 4000 --env api-target=http://localhost:8080",
    "test": "tsc --noEmit",
    "build": "webpack --config webpack.prod.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src --check",
    "docker:build": "./scripts/build_docker_image.sh",
    "docker:start-services": "docker-compose -f docker/hoppingmode-web.compose.yml -p hoppingmode-web up -d",
    "docker:start-api": "npx mattgoespro@latest hw run -i api --rm",
    "docker:remove-services": "docker-compose -f docker/hoppingmode-web.compose.yml -p hoppingmode-web down",
    "install-deps": "npm install",
    "prepare": "husky install"
  },
  "dependencies": {
