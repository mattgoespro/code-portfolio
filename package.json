    "babel-loader": "^8.2.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "eslint": "^8.20.0",
