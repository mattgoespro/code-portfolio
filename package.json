  "scripts": {
    "start:isolated": "webpack serve --config webpack.dev.js --mode development --port 4000",
    "start:dev": "curl localhost:8081 && webpack serve --config webpack.dev.js --mode development --port 4000 --env api-target=stub",
    "start:prod": "curl localhost:8080 && webpack serve --config webpack.prod.js --mode production --port 4000 --env api-target=api",
    "build": "webpack --config webpack.prod.js --mode production",
    "lint": "eslint src",
    "lint:fix": "concurrently \"eslint src --fix\" \"prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc\"",
    "lint:style-class-autoprefix": "postcss src/**/*.scss --use autoprefixer --replace --no-map",
    "docker:build": "docker build . -t hoppingmode-web/frontend",
    "docker:start-services": "docker-compose -f docker/hoppingmode-web.compose.yml -p hoppingmode-web up -d"
  },
