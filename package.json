{
  "name": "hoppingmode-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/markdown-it": "^12.2.3",
    "axios": "^0.27.2",
    "html-react-parser": "^1.4.8",
    "markdown-it": "^12.3.2",
    "mini-css-extract-plugin": "^2.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-horizontal-scrolling-menu": "^3.2.0",
    "react-loading-overlay-ts": "^2.0.0",
    "react-minimal-pie-chart": "^8.2.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.3",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --mode development --port 4000 --env api-target=http://localhost:8080",
    "start:prod": "webpack-dev-server --config webpack.prod.js --mode production --port 4000 --env api-target=http://localhost:8080",
    "build": "webpack --config webpack.prod.js --mode production",
    "lint": "eslint src",
    "lint:fix": "concurrently \"eslint src --fix\" \"prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc\"",
    "lint:gen-css-autoprefix": "postcss src/**/*.scss --use autoprefixer --replace --no-map",
    "docker:build": "docker build . -t hoppingmode-web/frontend",
    "docker:start-services": "docker-compose -f docker/hoppingmode-web.compose.yml -p hoppingmode-web up -d",
    "docker:remove-services": "docker-compose -f docker/hoppingmode-web.compose.yml -p hoppingmode-web down",
    "clean-build": "rm -rf dist",
    "clean-node": "rm -rf node_modules"
  },
  "devDependencies": {
